<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:XiaomiSoftwareManager"
	xmlns:UIComponents="clr-namespace:XiaomiSoftwareManager.UIComponents">


	<Style TargetType="{x:Type UIComponents:AppConsole}">
        <Setter Property="Template">
            <Setter.Value>
				<ControlTemplate TargetType="{x:Type UIComponents:AppConsole}">
                    <Border Background="{TemplateBinding Background}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            BorderThickness="{TemplateBinding BorderThickness}">
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>
